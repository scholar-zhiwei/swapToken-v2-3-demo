{
  "name": "imp-contract",
  "version": "0.0.1",
  "description": "imp contract project",
  "main": "index.js",
  "scripts": {
    "forkMain": "npx hardhat node --fork https://eth-mainnet.alchemyapi.io/v2/iP4C05RwgWU2zyWxx9oQYhb52WTNXnfR",
    "build": "cross-env TS_NODE_TRANSPILE_ONLY=1 hardhat compile",
    "gen-root": "cross-env TS_NODE_TRANSPILE_ONLY=1 hardhat gen-merkle-root",
    "clean": "cross-env TS_NODE_TRANSPILE_ONLY=1 hardhat clean",
    "test": "yarn build && hardhat test",
    "flat": "hardhat flatten ./contracts/IMP.sol > ./flat/IMP.sol",
    "kovan:deploy": "hardhat --network kovan deploy",
    "kovan:export": "hardhat --network kovan export --export exports/kovan.json",
    "release": "yarn version --prerelease && yarn publish"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@uniswap/v2-core": "^1.0.1"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.6.4",
    "@ethersproject/providers": "^5.6.8",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.2",
    "@nomicfoundation/hardhat-toolbox": "^1.0.1",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@openzeppelin/contracts": "^4.7.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.3",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.9",
    "hardhat": "^2.10.1",
    "hardhat-deploy": "^0.11.11",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "merkletreejs": "^0.2.32",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.8.2",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  }
}
